{
  "extends": "airbnb-base",
  "rules": {
    "no-unused-vars": "warn",
    "consistent-return": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "object-curly-newline": ["error", { "consistent": true }]
  },
  "env": {
    "node": true,
    "es6": true
  }
}
